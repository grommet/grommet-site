{
  "name": "grommet-site",
  "version": "0.1.0",
  "main": "src/index.js",
  "description": "Site for Grommet v2",
  "homepage": "https://v2.grommet.io",
  "bugs": "https://github.com/grommet/grommet-site/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/grommet/grommet-site.git"
  },
  "license": "Apache-2.0",
  "engine-strict": true,
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "release-heroku": "babel-node --presets @babel/preset-env ./tools/release-heroku.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "build-pages": "cross-env NODE_ENV=production babel-node tools/build-static-site.js",
    "build-static": "yarn build && yarn build-pages",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test": "eslint src",
    "prettier": "pretty-quick --staged",
    "server": "http-server dist"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "core-js": "^3.17.3",
    "grommet": "https://github.com/grommet/grommet/tarball/stable",
    "grommet-icons": "^4.0.0",
    "grommet-styles": "^0.2.0",
    "grommet-theme-aruba": "^0.1.2",
    "grommet-theme-dxc": "^0.1.2",
    "grommet-theme-hp": "^0.1.2",
    "grommet-theme-hpe": "^2.1.0",
    "json-stringify-pretty-compact": "^3.0.0",
    "lz-string": "^1.4.4",
    "prop-types": "^15.5.10",
    "react": "^17.0.2",
    "react-desc": "^4.0.0",
    "react-dom": "^17.0.2",
    "react-formify": "^0.11.0",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-is": "^17.0.2",
    "react-live": "2.3.0",
    "react-monaco-editor": "^0.44.0",
    "react-side-effect": "2.1.1",
    "styled-components": "^5.1.0",
    "url-search-params": "^1.0.2",
    "webfontloader": "^1.6.28",
    "webpack-manifest-plugin": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^11.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-parallel": "^1.0.0",
    "eslint-plugin-grommet": "^0.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.8.2",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^13.0.1",
    "monaco-editor": "0.27.0",
    "monaco-editor-webpack-plugin": "4.1.2",
    "node-fetch": "^3.0.0",
    "nodemon": "^2.0.12",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "react-dev-utils": "^11.0.4",
    "react-test-renderer": "^17.0.2",
    "simple-git": "^2.45.1",
    "ssri": "^8.0.1",
    "style-loader": "^3.2.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.0",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "pre-commit": [
    "lint-fix",
    "prettier"
  ],
  "sideEffects": false,
  "resolutions": {
    "react-helmet/react-side-effect": "2.1.0"
  }
}
